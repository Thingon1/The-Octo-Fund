<!-- @import "head.kit" -->

<body>
    <main>

        <div class="logo" style="display:none;">
            <div class="i-logo"></div>
            <h1>
                THE OCTO FUND
            </h1>
        </div>

        <section id="login" style="display:none;">

            <div class="access-code">
                <p class="instruction">
                    Please enter your access code below:
                </p>

                <form>
                    <input type="number" pattern="[0-9]*" maxlength="10"/>
                </form>

                <p class="get-code">
                    Don't have an access code? <br>
                    <button class='js-toggle-request'>Request one here</button>
                </p>
            </div>

            <p class="tagline">
                Private investment group, focused on the discretionary application of contextually relevant algorithmic strategies within the financial markets. <button class='js-toggle-request'>Learn more</button>
            </p>
            
        </section>

        <section id="request" style="display:none;">
            <div class="rq-bg js-toggle-request"></div>

            <button class='js-toggle-request'>
                X
            </button>

            <div class="request-content">

                <form class="content">
                    
                    <h3>
                        Request Access Code
                    </h3>

                    <p>
                        The Octo Fund makes the site, its content, details of the fund, and its performance available only to current clients and prospective investors. Please complete and submit the form below in order to request a unique access code.
                    </p>

                </form>
                
            </div>
        </section>

        <section id="welcome-alert" style="display:none;">
            <h3>
                Welcome, <span class="js-custom-name"></span>.
            </h3>
        </section>   

        <section id='social' style="display:none;">
            
            <div class="content">
                <h3>
                    Follow Us
                </h3>

                <button class="back-to-top">
                    ^ Back To Top
                </button>

                <a class="twitter-timeline" data-theme="light" href="https://twitter.com/TheOctoFund?ref_src=twsrc%5Etfw">Tweets by TheOctoFund</a> <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
            </div>

        </section>

        <section id="loading">
            <div class="i-logo"></div>
        </section>

        <section id="intro" style="display:none;">

            <div class="top">
                <h2>
                    52.08%
                </h2>
            </div>

            <div class="bottom">
                <hr>
                 <p class="intro-desc">    
                    YTD Performance
                </p>
            </div>

            <p class="disclaimer">
                Based on a 75% risk profile, updated monthly.
            </p>
            
        </section>
        
     </main>

<!--Javascript-->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script src="codes.js"></script>
<script src="main.js"></script>


<script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.9-1/crypto-js.min.js" integrity="sha384-lp4k1VRKPU9eBnPePjnJ9M2RF3i7PC30gXs70+elCVfgwLwx1tv5+ctxdtwxqZa7" crossorigin="anonymous"></script>
    
</body>
</html>
